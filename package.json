{
  "name": "bikeshop",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@7.26.0",
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}\n[ci skip]",
    "tagPrefix": ""
  },
  "scripts": {
    "prepare": "husky install && nx g @nx-dotnet/core:restore",
    "dev": "nx serve client",
    "build": "nx build client",
    "type-check": "tsc -b packages/client/tsconfig.app.json --incremental",
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test",
    "clean": "nx run-many --target=clean",
    "affected:lint": "nx affected:lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "register-local-nx-plugins": "nx run-many --target=build-plugin --all && nx run-many --target=link-plugin --all"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "aggregate-error": "4.0.0",
    "debug": "4.3.3",
    "execa": "5.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt-cli": "^17.4.2",
    "@emotion/babel-plugin": "11.10.5",
    "@nrwl/cli": "13.7.1",
    "@nrwl/cypress": "15.6.3",
    "@nrwl/devkit": "13.7.1",
    "@nrwl/eslint-plugin-nx": "13.7.1",

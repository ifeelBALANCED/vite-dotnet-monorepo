    "@nx-dotnet/core": "^1.18.1",
    "@nxext/vite": "^15.0.0",
    "@nxext/vitest": "^15.0.0",
    "@swc/core": "^1.3.30",
    "@testing-library/react": "13.4.0",
    "@types/bluebird": "3.5.38",
    "@types/jest": "27.0.2",
